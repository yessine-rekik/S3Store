{
  "extends": ["next/core-web-vitals", "eslint:recommended"],

  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "indent": ["error", 2],
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
    "no-multi-spaces": "error",
    "no-console": "error"
  },

  "env": {
    "es6": true
  }
}
